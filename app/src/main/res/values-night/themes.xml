<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.UniuyoHCPatientDetails" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color.   -->
        <item name="colorPrimary">@color/purple_200</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. ?attr/colorPrimaryVariant-->
        <item name="android:statusBarColor" >?attr/backgroundColor</item>
        <!-- Customize your theme here. -->
        <item name="android:autofilledHighlight" tools:targetApi="o">@android:color/transparent</item>
        <item name="backgroundColor">#0367D7</item>
        <item name="android:navigationBarColor">@color/transparent</item>


    </style>
    <style name="Theme.UniuyoHCPatientDetails.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>

        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>

    </style>
</resources>